{
  "compilerOptions": {
    "charset": "utf-8",
    "declaration": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react",
    "lib": [
      "es2016"
    ],
    "module": "commonjs",
    "newLine": "LF",
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "sourceMap": true,
    "strictNullChecks": true,
    "outDir": "./dist/",
    "pretty": true,
    "target": "es5"
  },
  "include": [
    "src/**/*"
  ]
}
